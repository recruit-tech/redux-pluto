  "version": "0.0.0",
  "private": true,
  "main": "src/server/server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean build:{client,server}",
    "build:client": "webpack --verbose --colors --display-error-details --config src/webpack/prod.client.config.js",
    "build:server": "webpack --verbose --colors --display-error-details --config src/webpack/prod.server.config.js",
    "clean": "rimraf build/*",
    "clean-start": "npm-run-all clean build start:prod",
    "eslint": "eslint src",
    "fixpack": "fixpack",
    "jscs": "jscs src",
    "lint": "npm-run-all eslint jscs",
    "prod": "npm-run-all build prod:start",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development npm-run-all --parallel start:dev:{webpack,nodemon}",
    "start:dev:nodemon": "nodemon --exec npm run start:dev:server",
    "start:dev:server": "cross-env PORT=3001 DEBUG=express:*,app:* babel-node bin/start-server.js",
    "start:dev:webpack": "webpack-dev-server --hot --inline --config src/webpack/dev.client.config.js",
    "start:prod": "cross-env NODE_ENV=production npm run start:prod:server",
    "start:prod:server": "node bin/start-server",
    "test": "eater"
  },
