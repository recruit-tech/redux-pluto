  "greenkeeper": {
    "ignore": [
      "react-router"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/client/bootstrap-*.js.gz",
      "maxSize": "150 kB"
    },
    {
      "path": "./build/client/bootstrap-*.js.br",
      "maxSize": "130 kB"
    },
    {
      "path": "./build/client/main-*.js.gz",
      "maxSize": "20 kB"
