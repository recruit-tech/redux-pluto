{
  "name": "redux-proto",
  "description": "redux-proto",
  "version": "0.0.0",
  "private": true,
  "main": "src/server/server.js",
  "scripts": {
    "analytics": "sonarish src",
    "build": "cross-env NODE_ENV=production RS_ENV=prd run-s clean build:{client,server,size}",
    "build:client": "webpack --verbose --colors --display-error-details --config src/webpack/prod.client.config.js",
    "build:csr": "cross-env DISABLE_SSR=1 npm run build",
    "build:link-css": "cross-env DISABLE_INLINE_CSS=1 npm run build",
    "build:offload": "cross-env ENABLE_OFFLOAD=1 npm run build",
    "build:server": "webpack --verbose --colors --display-error-details --config src/webpack/prod.server.config.js",
    "build:size": "webpack-bundle-size-analyzer build/client/stats.json",
    "build:mock": "run-p -r start:dev:mock build:mock:wait-on",
    "build:mock:wait-on": "wait-on tcp:3000 && node ./mock/src/index",
    "bundlesize": "npx bundlesize",
    "checksize": "run-s build bundlesize",
    "clean": "rimraf build/*",
    "clean-start": "run-s clean build start:prod",
    "fix": "run-s fix:*",
    "fix:js": "eslint 'src/**/*.{ts,tsx}' --fix && prettier 'src/**/*.{ts,tsx}' --write",
    "lint": "run-s lint:*",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "lint:style": "stylelint src",
    "prod": "run-s clean build start:prod",
    "typecheck": "tsc -p . --noEmit",
    "start": "npm run start:dev",
    "start:csr": "cross-env DISABLE_SSR=1 npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development run-p start:dev:{server,agreed}",
    "start:dev:agreed": "agreed-server --path ./spec/agreed/agreed.ts --port 3010",
    "start:dev:server": "cross-env DEBUG=express:*,app:* node bin/start-server.js",
    "start:dev:mock": "MOCK_BUILD=true npm run start:dev",
    "start:mock": "node ./mock/src/static-server",
    "start:offload": "cross-env ENABLE_OFFLOAD=1 npm run start:dev",
    "start:prod": "cross-env NODE_ENV=production run-p start:{prod:server,dev:agreed}",
    "start:prod:server": "node bin/start-server.js",
    "test": "run-p test:agreed test:wait-on -r",
    "test:agreed": "agreed-server --path ./spec/agreed/agreed.ts --port 3020",
    "test:old": "run-p test:agreed test:wait-on -r --print-label",
    "test:wait-on": "wait-on tcp:3020 && jest --maxWorkers=4",
    "test:coverage": "jest --coverage --maxWorkers=4 && cat ./coverage/lcov.info | codacy-coverage",
    "gen:schema": "agreed-typed gen-swagger --path ./spec/agreed/agreed.ts"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.0",
