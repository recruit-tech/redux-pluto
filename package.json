    "hoist-non-react-statics": "^3.3.0",
    "is-redirectable-url": "^1.0.2",
    "isomorphic-form-data": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash.isequal": "^4.5.0",
    "ms": "^2.0.0",
    "multer": "^1.3.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
