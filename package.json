    "agreed": "^3.2.0",
    "assert-stream": "^1.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "1.9.2",
    "babel-plugin-transform-imports": "^1.5.0",
