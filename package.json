    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router": "3.2.0",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.4",
    "react-universal-component": "^4.0.0",
    "react-virtualized": "^9.21.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-async-loader": "^1.2.4",
    "redux-effects-fetchr": "^1.1.0",
    "redux-effects-fetchr-cache": "^1.0.1",
    "redux-effects-formdata-uploader": "^1.2.3",
    "redux-effects-steps": "^1.0.1",
    "redux-form": "^8.2.3",
    "reselect": "^4.0.0",
